{
  "name": "indonesia-covid-api",
  "version": "1.0.0",
  "description": "Static server to provide the latest Coronavirus / Covid 19 data in Indonesia",
  "keywords": [
    "Javascript",
    "Node"
  ],
  "author": "Rfzsal",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build": "node --icu-data-dir=node_modules/full-icu build"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "cheerio": "^1.0.0-rc.3",
    "dotenv": "^8.2.0",
    "full-icu": "^1.3.1",
    "luxon": "^1.25.0"
  },
  "devDependencies": {
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.1.2"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "eslintConfig": {
    "env": {
      "commonjs": true,
      "es2021": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 12
    },
    "rules": {}
  },
  "engines": {
    "node": "12.x"
  }
}
